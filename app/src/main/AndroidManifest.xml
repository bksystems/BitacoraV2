<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.movilidadcb.bitacorav2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".Entitys.UserGlobalClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppThemeCompartamos">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VersionActivity" />
        <activity android:name=".BaseDeDatosActivity" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".AcercaDeActivity" />
        <activity
            android:name=".DetalleVisitaActivity"
            android:label="@string/title_activity_detalle_visita"
            android:theme="@style/AppThemeCompartamos" />
        <activity
            android:name=".MainActivityNavigationDrawer"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_main_navigation_drawer"
            android:theme="@style/AppThemeCompartamos" />
        <activity android:name=".PlCamposActivity"></activity>
    </application>

</manifest>